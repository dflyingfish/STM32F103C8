     88            GPIO_InitStructure.GPIO_Speed = GPIO_Speed_10MHz;
     89            GPIO_InitStructure.GPIO_Mode = GPIO_Mode_Out_PP;
     90            GPIO_Init(CLASS_PARAMS->hDOutputPort, &GPIO_InitStructure);
     91            DOUT_SetOutputState(this, INACTIVE);
     92          }
