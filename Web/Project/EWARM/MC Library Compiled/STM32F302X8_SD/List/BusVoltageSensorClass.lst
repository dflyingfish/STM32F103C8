    137            uint16_t VBS_GetAvBusVoltage_V(CVBS this)
    138            {
    139            uint32_t wTemp;
    140            
    141            wTemp = (uint32_t)(BCLASS_VARS.hAvBusVoltage_d);
    142            wTemp *= BCLASS_PARAMS->hConversionFactor;
    143            wTemp /= 65536u;
    144            
    145            return((uint16_t)wTemp);
    146            }
